{
  "schema": {
    "$id": "https://nga.com/oe.request-task-profile-custom.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Custom RTP Schema",
    "description": "Example custom schema showing off extra questions with the core baseSchema contained within",
    "createdBy": "Billy Larsen",
    "createdFor": "RTP",
    "version": ".01",
    "type": "object",
    "properties": {
      "favoriteEditor": {
        "type": "string",
        "description": "Which Editor do you prefer the most",
        "enum": [
          "JSON",
          "YAML",
          "XML",
          "HCL",
          "None"
        ]
      },
      "leastEditor": {
        "type": "string",
        "description": "Which Editor do you prefer the least",
        "enum": [
          "JSON",
          "YAML",
          "XML",
          "HCL",
          "None"
        ]
      },
      "improvements": {
        "type": "string",
        "description": "What kinds of improvements can we make to the RTP Service"
      },
      "nested": {
        "type": "object",
        "properties": {
          "userNm": {
            "type": "string"
          }
        }
      },
      "baseSchema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "$id": "https://example.com/logging.schema.json",
        "meta": {
          "id": 1234,
          "createdAt": 1658179461,
          "schemaId": "Machine_Feedback",
          "version": "0.1",
          "title": "Machine Feedback Core",
          "description": "This is a test RTP schema",
          "author": "Joe HMF",
          "service": [
            "HMF"
          ],
          "originSchema": ""
        },
        "title": "HMFEvent",
        "description": "An event through the HMF System",
        "type": "object",
        "properties": {
          "serviceId": {
            "description": "High Leve service family of the log //TODO Do we want to leave this as an enum or use an uuid?",
            "enum": [
              "HMF",
              "EVENT",
              "RTP",
              "ORCHESTRATON",
              "UI",
              "SESSION",
              "USER"
            ]
          },
          "eventTypeCd": {
            "description": "Runtime of this event",
            "enum": [
              "EVENT_START",
              "EVENT_IN_PROGRESS",
              "EVENT_END",
          	  "EVENT_INFO"
            ]
          },
          "logLevel": {
            "description": "Log Level of event",
            "enum": [
              "FATAL",
              "SYSTEM",
              "ERROR",
              "WARNING",
              "INFO",
              "DEBUG",
              "TRACE"
            ]
          },
          "userId": {
            "description": "User Id",
            "type": "string",
            "format": "uuid"
          },
          "orderId": {
            "description": "OrderId Generated by RTP",
            "type": "string",
            "format": "uuid"
          },
          "sessionId": {
            "description": "SessionId from SessionManager.  Will be passed forward, can determine Order/User from this",
            "type": "string",
            "format": "uuid"
          },
          "workflowId": {
            "description": "workflowId assigned in Orchestration.  Maybe available?",
            "type": "string",
            "format": "uuid"
          },
          "microServiceId": {
            "description": "The ID of the individual microservice for this message",
            "type": "string"
          },
          "uiTransactionId": {
            "description": "ID generated that matches to the Matomo instance",
            "type": "string"
          },
          "simulationFlg": {
            "description": "Is this a sumulation?  Should we log it?",
            "type": "boolean"
          },
          "executionDateTime": {
            "description": "Execution Start Date and Time that the event occurs in GMT",
            "type": "string",
            "format": "date-time"
          },
          "classificationLevel": {
            "description": "Level of Classification for schema.",
            "enum": [
              "UNCLASSIFIED",
              "SECRET",
              "TOP-SECRET"
            ]
          },
          "environment": {
            "description": "What env the service ran in",
            "enum": [
              "XC",
              "UC"
            ]
          },
          "escalationLevel": {
            "description": "Level of Escalation.",
            "type": "number"
          },
          "eventName": {
            "description": "Name or title of error",
            "type": "string"
          },
          "eventSummary": {
            "description": "Summary of the event.  Keep short as this will be displayed to user",
            "type": "string"
          },
          "payload": {
            "description": "Input Parameters of the Service (Env Variables) or Optional Payload of the data coming out of Service.",
            "type": "object"
          },
          "eventDetails": {
            "description": "Optional Longer Description if more information is provided",
            "type": "string"
          },
          "stackTrace": {
            "description": "Optional Stack Trace of the Event.  Will not be captured at all log levels",
            "type": "string"
          },
          "originalOrderId": {
            "description": "Optional Provided OrderId in event of a retry?",
            "type": "string",
            "format": "uuid"
          }
        },
        "required": [
          "logLevel",
          "serviceId",
          "eventTypeCd",
          "executionDateTime",
          "classificationLevel",
          "eventName",
          "sessionId",
          "microServiceId"
        ]
      }
    },
    "required": [
      "favoriteEditor",
      "leastEditor"
    ]
  },
  "data": {
    "favoriteEditor": "JSON",
    "leastEditor": "XML",
    "nested": {
      "userNm": "Billy"
    },
    "baseSchema": {
      "serviceId": "HMF",
      "logLevel": "SYSTEM",
      "userId": "6f57c44b-b84e-49e0-9f04-71d734dadc48",
      "sessionId": "13e303a7-0c40-447d-8601-f1b2391c2b24",
      "microServiceId": "machine:feedback:db",
      "simulationFlg": false,
      "executionDateTime": "2022-08-16T01:54:51.102Z",
      "classificationLevel": "UNCLASSIFIED",
      "environment": "XC",
      "escalationLevel": 0,
      "eventName": "Simulation Machine Log",
      "eventSummary": "Sample Machine Log Message",
      "eventTypeCd": "EVENT_START",
      "payload": {
        "msg": "Succesfully Connected to Simulation Server with UserId 1234",
        "state": "success"
      }
    }
  }
}